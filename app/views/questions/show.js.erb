<% @answerfeed.each do |answer| %>
$('#answers').append("<%= escape_javascript (render 'questions/answer', answer: answer,question_id: @question.id) %>");
<% end %>
<% @commentfeed.each do |comment| %>
$('#comments').append("<%= escape_javascript (render 'answers/comment', comment: comment,answer_id: @answer.id) %>");
<% end %>
